<!doctype html>
<html>
  <head>
    <title>Vincents super awesome playlist</title>
    <style>
      * { margin: 0; padding: 0; box-sizing: border-box; }
      body { font: 13px Helvetica, Arial; }
    </style>
  </head>
  <body>
    <ul>
      <% searches.forEach(function(search) { %>
        <li><%= search.artist %> - <%= search.title %></li>
      <% }); %>
    </ul>
  </body>
</html>
